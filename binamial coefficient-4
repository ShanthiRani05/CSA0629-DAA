
#include <stdio.h>

#define MAX 100

int binomialCoefficient(int n, int k) {
    int dp[MAX][MAX];
    
    int i, j; 
    for (i = 0; i <= n; i++) {
        for (j = 0; j <= k; j++) {
            if (j == 0 || j == i) {
                dp[i][j] = 1;
            } else if (j < i) {
                dp[i][j] = dp[i - 1][j - 1] + dp[i - 1][j];
            }
        }
    }

    return dp[n][k];
}

int main() {
    int n = 8, k = 8;
    printf("Binomial Coefficient C(%d, %d) = %d\n", n, k, binomialCoefficient(n, k));
    return 0;
}
